<!doctype html>
<!-- Project -->
<html lang="en">
{{> head }}
<body>

	{{> header }}

	<section class="main">
		
		<h1>{{ name }}</h1>

		<div>{{{ contents }}}</div>

		{{#if previous}}
		<a href='/projects/{{ previous.paths.name }}'>&lt; &lt; {{ previous.name }}</a>
		{{/if}}
		
		{{#if next}}
		<a href='/projects/{{ next.paths.name }}'>{{ next.name }} &gt; &gt;</a>
		{{/if}}

	</section>

	{{> footer }}

{{> scripts}}
</body>
</html>